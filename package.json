{
  "name": "expo-kokoro",
  "version": "1.0.6",
  "description": "A expo module for using kokoro tts models",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "jest",
    "prepare": "expo-module prepare && cp -r assets build/assets",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-kokoro",
    "Kokoro"
  ],
  "repository": "https://github.com/danemadsen/expo-kokoro",
  "bugs": {
    "url": "https://github.com/danemadsen/expo-kokoro/issues"
  },
  "author": "Dane Madsen <dane_madsen@hotmail.com> (https://github.com/danemadsen)",
  "license": "MIT",
  "homepage": "https://github.com/danemadsen/expo-kokoro#readme",
  "dependencies": {
    "expo-file-system": "^18.1.11",
    "onnxruntime-react-native": "^1.22.0",
    "expo": "~53.0.22",
    "react": "19.0.0",
    "react-native": "0.79.6"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.0",
    "expo": "~53.0.0",
    "expo-module-scripts": "^4.1.10",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "react-native": "0.79.1",
    "ts-jest": "^29.4.1"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*",
    "onnxruntime-react-native": "*"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx|js|jsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "isolatedModules": true,
          "diagnostics": true,
          "tsconfig": {
            "resolveJsonModule": true,
            "esModuleInterop": true,
            "allowSyntheticDefaultImports": true
          }
        }
      ]
    },
    "extensionsToTreatAsEsm": [".ts", ".tsx"]
  }
}
